---
import Navbar from '../components/Navbar.vue';
import Layout from '../layouts/Layout.astro';
import AnwarGallery from '../components/AnwarGallery.vue';
import ThejusContent from '../components/ThejusContent.astro';
import StaysContent from '../components/Stays/StaysContent.astro';
import Amenites from '../components/Amenites/Amenites.astro';

import data from '../data/propertydata.json';

import '@fontsource/prata';
import '@fontsource-variable/raleway';
import '@fontsource/poppins';
import '../styles/global.css';
import Review from '../components/Reviews/Review.astro';
import Contact from '../components/Contact.astro';
import Rudra from '../components/Rudra.astro';
import Footer from '../components/Footer.astro';
import Gallery from '../components/Gallery/Gallery.astro';
import Photoshoot from '../components/Photoshoot.astro';
import Celebrate from '../components/Celebrate/Celebrate.astro';
import Socials from '../components/Socials.astro';
---



<Layout >
	<Navbar client:load class="overflow-x-hidden"/>
	<AnwarGallery 
	client:load
    images={data.image}
    card={data}/>

	<div id="about" />
	<ThejusContent />
	
	<div id="stay" />
	<StaysContent />

	<div id="exp" />
	<Amenites />
	
	<Celebrate />
	
	<Photoshoot	/>
	
	<div id="gallery" />
	<Gallery />

	<Review/>
	
	<div id="contact" />
	<Contact/>

	<Rudra/>

	<Socials/>

	<Footer />

</Layout>
