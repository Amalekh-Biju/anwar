---
import Navbar from '../components/Navbar.vue';
import Layout from '../layouts/Layout.astro';
import TejusGallery from '../components/TejusGallery.vue';
import ThejusContent from '../components/ThejusContent.astro';
import StaysContent from '../components/Stays/StaysContent.astro';
import Amenites from '../components/Amenites/Amenites.astro';

import data from '../data/propertydata.json';

import '@fontsource/prata';
import '@fontsource-variable/raleway';
import '../styles/global.css';
import Activites from '../components/Activites/Activites.astro';
import Review from '../components/Reviews/Review.astro';
import Contact from '../components/Contact.astro';
import Rudra from '../components/Rudra.astro';
---



<Layout>
	<Navbar client:load/>
	<TejusGallery 
	client:load
    images={data.image}
    card={data}/>

	<div id="about" />
	<ThejusContent />
	
	<div id="stay" />
	<StaysContent />

	<div id="exp" />
	<Amenites />
	
	<Activites />
	
	<!-- TODO Gallery -->
	
	<Review/>
	
	<div id="contact" />
	<Contact/>

	<Rudra/>

</Layout>
