---
import Navbar from '../components/Navbar.astro';
import Layout from '../layouts/Layout.astro';
import TejusGallery from '../components/TejusGallery.vue';
import ThejusContent from '../components/ThejusContent.astro';
import StaysContent from '../components/StaysContent.astro';
import Amenites from '../components/Amenites.astro';

import data from '../data/propertydata.json';

import '@fontsource/prata';
import '@fontsource-variable/raleway';
import '../styles/global.css';
---



<Layout>
	<Navbar />
	<TejusGallery 
	client:load
    images={data.image}
    card={data}/>

	<ThejusContent />

	<StaysContent />

	<Amenites />

</Layout>
